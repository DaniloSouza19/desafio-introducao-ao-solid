{
  "openapi": "3.0.0",
  "info": {
    "title": "Gerenciamento de usuários",
    "description": "É uma api para gerenciamento de usuários",
    "version": "1.0.0"
  },
  "paths": {
    "/users": {
      "post": {
        "tags": ["User"],
        "summary": "Create an User",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "John Doe",
                  "email": "johndoe@example.com"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "created"
          },
          "400": {
            "description": "Email Already exists"
          }
        }
      }
    }
  }
}